{
  "_comment_config": "CONFIGURACIÓN CORREGIDA - El problema estaba en metric_thresholds que contenía claves de exclusions",
  
  "experiment": {
    "name": "MOEAD_Climate_5Obj_WorldReady",
    "description": "Optimización multiobjetivo clima-industria con MOEA/D - Valores Continuos",
    "scenario": "climate_5_obj",
    "random_seed": 42,
    "output_root": "results"
  },
  
  "dataset": {
    "_comment_data_strategy": "Se definen dos rutas de datos para mantener la armonía del sistema",
    "name": "climate_energy",
    "processed_path": "data/processed/climate_dataset/climate_dataset_processed.csv",
    "raw_path": "data/raw/climate_dataset/global_climate_energy_2020_2024.csv",
    "supports_path": "data/processed/climate_dataset/supports.json",
    "metadata_path": "data/processed/climate_dataset/metadata.json"
  },
  
  "use_sampling": false,
  
  "algorithm": {
    "generations": 300,
    "population_size": 126,
    "logging_interval": 10,
    
    "termination": {
      "enabled": true,
      "ftol": 1e-06,
      "period": 50
    },
    
    "initialization": {
      "use_pregenerated": false,
      "max_attempts": 20000
    },
    
    "decomposition": {
      "method": "augmented_tchebycheff",
      "params": { 
        "rho": 0.01
      }
    },
    
    "neighborhood": {
      "size": 20,
      "replacement_size": 2,
      "selection_probability": 0.9
    },
    
    "operators": {
      "crossover": {
        "probability": {
          "initial": 0.9,
          "min": 0.6,
          "max": 1.0
        }
      },
      "mutation": {
        "method": "conservative",
        "active_ops": ["extension", "contraction", "replacement"],
        "probability": {
          "initial": 0.4,
          "min": 0.1,
          "max": 0.8
        },
        "repair_attempts": 20,
        "duplicate_attempts": 20,
        "reproducible_mode": true,
        "max_operations": 3000,
        "timeout": 10.0,
        "timeout_per_attempt": 25.0
      }
    },
    
    "stuck_detection": {
      "enabled": true,
      "window": 50,
      "min_new": 5,
      "hv_window": 50,
      "hv_tol": 1e-06
    }
  },
  
  "objectives": {
    "selected": [
      "co2_emission",
      "energy_consumption",
      "renewable_share",
      "industrial_activity_index",
      "energy_price"
    ],
    "directions": {
      "co2_emission": "minimize",
      "energy_consumption": "minimize",
      "renewable_share": "maximize",
      "industrial_activity_index": "maximize",
      "energy_price": "minimize"
    }
  },
  
  "constraints": {
    "rule_validity": {
      "min_antecedent_items": 1,
      "min_consequent_items": 1,
      "max_antecedent_items": 5,
      "max_consequent_items": 3
    },
    "metric_thresholds": {
      "_comment": "CORREGIDO: Este objeto debe estar VACÍO o contener solo umbrales de métricas reales"
    },
    "exclusions": {
      "_comment": "Las exclusiones van aquí, NO en metric_thresholds",
      "fixed_antecedents": [],
      "fixed_consequents": [],
      "forbidden_pairs": []
    }
  },
  
  "hypervolume": {
    "reference_point": "nadir",
    "nadir_value": 1.1,
    "filter_invalid": true,
    "invalid_threshold": 1.5
  },
  
  "visualization": {
    "enabled": true,
    "parallel_coordinates": true,
    "pareto_3d_subset": ["co2_emission", "renewable_share", "industrial_activity_index"],
    "evolution_plots": true
  }
}