{
    "dataset_name": "Climate Energy 2020-2024",
    "target_variable": {
      "name": "energy_price",
      "type": "ordinal",
      "cardinality": 5
    },
    "feature_order": [
      "country",
      "avg_temperature",
      "humidity",
      "co2_emission",
      "energy_consumption",
      "renewable_share",
      "urban_population",
      "industrial_activity_index",
      "energy_price"
    ],
    "categorical_encoding": {
      "country": {
        "type": "nominal",
        "mapping": {
          "Australia": 0, "Brazil": 1, "Canada": 2, "China": 3, "France": 4,
          "Germany": 5, "India": 6, "Indonesia": 7, "Italy": 8, "Japan": 9,
          "Mexico": 10, "Netherlands": 11, "Norway": 12, "Poland": 13,
          "South Africa": 14, "Spain": 15, "Sweden": 16, "Turkey": 17,
          "United Kingdom": 18, "United States": 19
        },
        "cardinality": 20
      }
    },
    "continuous_to_categorical": {
      "avg_temperature": {
        "type": "ordinal", "cardinality": 5,
        "bins": [-999999999.0, 3.98, 10.53, 16.44, 22.62, 999999999.0],
        "labels": ["Very Low", "Low", "Medium", "High", "Very High"], "right": true
      },
      "humidity": {
        "type": "ordinal", "cardinality": 5,
        "bins": [-999999999.0, 41.98, 54.01, 66.0, 78.02, 999999999.0],
        "labels": ["Very Low", "Low", "Medium", "High", "Very High"], "right": true
      },
      "co2_emission": {
        "type": "ordinal", "cardinality": 5,
        "bins": [-999999999.0, 213.81, 354.08, 491.59, 675.60, 999999999.0],
        "labels": ["Very Low", "Low", "Medium", "High", "Very High"], "right": true
      },
      "energy_consumption": {
        "type": "ordinal", "cardinality": 5,
        "bins": [-999999999.0, 3652.72, 5810.44, 7984.73, 10904.96, 999999999.0],
        "labels": ["Very Low", "Low", "Medium", "High", "Very High"], "right": true
      },
      "renewable_share": {
        "type": "ordinal", "cardinality": 5,
        "bins": [-999999999.0, 11.09, 14.24, 17.23, 20.81, 999999999.0],
        "labels": ["Very Low", "Low", "Medium", "High", "Very High"], "right": true
      },
      "industrial_activity_index": {
        "type": "ordinal", "cardinality": 5,
        "bins": [-999999999.0, 52.22, 64.22, 76.09, 88.17, 999999999.0],
        "labels": ["Very Low", "Low", "Medium", "High", "Very High"], "right": true
      },
      "energy_price": {
        "type": "ordinal", "cardinality": 5,
        "bins": [-999999999.0, 64.19, 98.20, 132.29, 166.65, 999999999.0],
        "labels": ["Very Low", "Low", "Medium", "High", "Very High"], "right": true
      }
    }
  }